{
  "version": "independent",
  "npmClient": "yarn",
  "ignoreChanges": ["**/__fixtures__/**", "**/__tests__/**", "**/*.md"],
  "command": {
    "version": {
      "allowBranch": ["master", "feature/*"],
      "message": "chore(release): publish %s"
    }
  },
  "packages": [
    "packages/*"
  ]
}
